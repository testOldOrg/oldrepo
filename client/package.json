    "enzyme-adapter-react-16": "^1.7.1",
    "handlebars": "^4.5.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^23.6.0",
    "jest-fetch-mock": "^2.1.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "regenerator-runtime": "^0.13.3",
    "webpack": "^4.41.6",
    "webpack-dev-server": "^3.1.14"
