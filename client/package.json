{
  "scripts": {
    "test": "jest",
    "devClient": "webpack-dev-server --mode development --devtool 'inline-cheap-module-source-map' --env.CLIENT_PORT=3000 --env.SERVER_PORT=31400 --env.LOG_LEVEL=5",
    "prodClient": "webpack --mode production --env.LOG_LEVEL=1",
    "server": "java -Dorg.slf4j.simpleLogger.log.com.tco=info -jar ../target/server-*.jar 31400",
    "dev": "concurrently \"npm run server\" \"npm run devClient\""
  },
  "dependencies": {
    "@material-ui/core": "^4.9.5",
    "ajv": "^6.12.0",
    "bootstrap": "^4.4.1",
    "leaflet": "^1.6.0",
    "node-sass": "^4.13.1",
    "notistack": "^0.9.9",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-leaflet": "^2.6.1",
    "reactstrap": "^8.4.1",
    "ulog": "^1.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.8.3",
    "@babel/plugin-transform-async-to-generator": "^7.8.3",
    "@babel/plugin-transform-object-assign": "^7.8.3",
    "@babel/polyfill": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-react": "^7.8.3",
    "@babel/runtime": "^7.8.4",
    "@babel/standalone": "^7.8.4",
    "autoprefixer": "^9.7.4",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^25.1.0",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "concurrently": "^5.1.0",
    "css-loader": "^3.4.2",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^6.8.0",
    "file-loader": "^5.1.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^25.1.0",
    "jest-cli": "^25.1.0",
    "jest-fetch-mock": "^3.0.1",
    "postcss-loader": "^3.0.0",
    "precss": "^4.0.0",
    "regenerator-runtime": "^0.13.3",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.1.3",
    "webpack": "^4.41.6",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/**.{js,jsx}"
    ],
    "coveragePathIgnorePatterns": [
      "/coverage/*",
      "/dist/*",
      "/node_modules/*",
      "/schemas/*",
      "/templates/*",
      "/test/*",
      "/webpack.config.js"
    ],
    "coverageDirectory": "../target/coverage/client/",
    "testURL": "http://localhost/",
    "moduleNameMapper": {
      "^.+\\.(png|svg|gif|jpg|jpeg)$": "<rootDir>/test/jestConfig/require_stub.js",
      "^.+\\.(css|scss)$": "<rootDir>/test/jestConfig/require_stub.js"
    },
    "automock": false,
    "setupFiles": [
      "<rootDir>/test/jestConfig/setupJest.js"
    ],
    "transform": {
      "^.+\\.jsx$": "babel-jest",
      "^.+\\.js$": "babel-jest"
    }
  }
}
