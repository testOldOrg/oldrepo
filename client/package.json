{
  "scripts": {
    "test": "jest",
    "devClient": "webpack-dev-server --mode development --devtool 'inline-cheap-module-source-map' --env.CLIENT_PORT=3000 --env.SERVER_PORT=31400 --env.LOG_LEVEL=5",
    "prodClient": "webpack --mode production --env.LOG_LEVEL=1",
    "server": "java -Dorg.slf4j.simpleLogger.log.com.tco=info -jar ../target/server-*.jar 31400",
    "dev": "concurrently \"npm run server\" \"npm run devClient\"",
    "prod": "npm run prodClient"
  },
  "dependencies": {
    "bootstrap": "^4.2.1",
    "css-loader": "^2.1.0",
    "file-loader": "^3.0.1",
    "leaflet": "^1.4.0",
    "react-leaflet": "^2.2.0",
    "reactstrap": "^7.0.2",
    "style-loader": "^0.23.1",
    "ulog": "^1.1.0",
    "webpack": "^4.28.4",
    "webpack-cli": "^3.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/polyfill": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-react": "^7.8.3",
    "ajv": "^6.10.2",
    "babel-jest": "^22.2.2",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "concurrently": "^5.1.0",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^23.6.0",
    "jest-fetch-mock": "^2.1.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "webpack-dev-server": "^3.1.14"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{js|jsx}"
    ],
    "testURL": "http://localhost/",
    "coverageDirectory": "coverage",
    "moduleNameMapper": {
      "\\.(png|svg|gif|jpg|jpeg)$": "<rootDir>/test/require_stub.js",
      "\\.(css)$": "<rootDir>/test/require_stub.js"
    },
    "automock": false,
    "setupFiles": [
      "<rootDir>/test/setupJest.js"
    ]
  }
}
